{
  "name": "@contextascode/cli",
  "version": "0.1.0",
  "description": "Command-line interface for Context as Code",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "codex": "dist/codex.js",
    "contextaider": "dist/contextaider.js"
  },
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "postbuild": "chmod +x dist/codex.js dist/contextaider.js",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "context",
    "documentation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^13.1.0",
    "enquirer": "^2.4.1"
  }
}
